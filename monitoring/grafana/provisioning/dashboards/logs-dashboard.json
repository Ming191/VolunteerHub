            "targets": [
                {
                    "datasource": {
                        "type": "loki",
                        "uid": "loki"
                    },
                    "expr": "{service=\"backend\"} | json | line_format \"{{.level | upper}} [{{.logger_name}}] {{.message}}\"",
                    "refId": "A"
